{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noarchive"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index-apex.html"
    },
    {
      "source": "/index-apex.html",
      "destination": "/index-apex.html"
    }
  ]
}
